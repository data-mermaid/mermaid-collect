  "engines": {
    "node": ">=8.9.0 <9.0",
    "npm": "~6.4.1"
  },
  "author": "MERMAID",
  "license": "MERMAID",
  "devDependencies": {
    "angular-mocks": "^1.4.14",
    "bower": "^1.8.8",
    "event-stream": "~3.3.1",
    "file-exists": "^5.0.1",
    "gulp": "^4.0.0",
    "gulp-angular-templatecache": "^1.7.0",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-jshint": "^2.0.4",
    "gulp-livereload": "^3.8.1",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-replace": "^0.5.4",
    "gulp-rev-all": "^0.9.7",
    "gulp-rev-delete-original": "^0.2.2",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.8",
    "husky": "^1.1.3",
    "jasmine-core": "^3.3.0",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.19",
    "prettier": "^1.15.3",
    "sw-precache": "^5.2.1"
  },
  "dependencies": {
    "angular-auth0": "^3.0.4",
    "leaflet": "^0.7.1",
    "leaflet-gesture-handling": "^1.1.8",
    "mapbox-gl": "^1.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
